<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.app.emp.EmpMapper">
<select id="getEmp"
parameterType="com.yedam.app.emp.EmpVO"
resultType="com.yedam.app.emp.EmpVO">
SELECT *
FROM employees
WHERE employee_id = #{employee_id}
</select>

</mapper>